{
	"easycom": {
		"^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "玩安卓",
			"enablePullDownRefresh": true
		}
	}, {
		"path": "pages/mine/mine",
		"style": {
			"navigationStyle": "custom",
			"enablePullDownRefresh": false
		}
	}, {
		"path": "pages/webView/webView",
		"style": {
			"navigationStyle": "custom",
			"enablePullDownRefresh": false
		}
	}, {
		"path": "pages/login/login",
		"style": {
			"navigationBarTitleText": "",
			"enablePullDownRefresh": false
		}

	}, {
		"path": "pages/register/register",
		"style": {
			"navigationBarTitleText": "",
			"enablePullDownRefresh": false
		}

	}, {
		"path": "pages/tree/tree",
		"style": {
			"enablePullDownRefresh": false,
			"navigationBarTitleText": "知识体系"
		}

	}, {
		"path": "pages/project/project",
		"style": {
			"navigationBarTitleText": "项目分类",
			"enablePullDownRefresh": false
		}

	}],
	"subPackages": [{
		"root": "pages/tree/articlePage",
		"pages": [{
			"path": "article",
			"style": {
				"navigationBarTitleText": "文章列表",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "searchArticle",
			"style": {
				"navigationBarTitleText": "搜索结果",
				"enablePullDownRefresh": false
			}

		}]
	}, {
		"root": "pages/project/projectList",
		"pages": [{
			"path": "list",
			"style": {
				"navigationBarTitleText": "项目列表",
				"enablePullDownRefresh": false
			}

		}]
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"background": "#efeff4"
		}
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#007AFF",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/icon/index.png",
			"selectedIconPath": "static/icon/index_s.png",
			"text": "首页"
		}, {
			"pagePath": "pages/tree/tree",
			"iconPath": "static/icon/tixi.png",
			"selectedIconPath": "static/icon/tixi_s.png",
			"text": "体系"
		}, {
			"pagePath": "pages/project/project",
			"iconPath": "static/icon/project.png",
			"selectedIconPath": "static/icon/project_s.png",
			"text": "体系"
		}, {
			"pagePath": "pages/mine/mine",
			"iconPath": "static/icon/mine.png",
			"selectedIconPath": "static/icon/mine_s.png",
			"text": "我的"
		}]
	}
}
